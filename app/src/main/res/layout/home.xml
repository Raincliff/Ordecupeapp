<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/BTNWHI"
        android:layout_width="114dp"
        android:layout_height="93dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true"
        android:layout_marginStart="134dp"
        android:layout_marginTop="16dp"
        android:background="#ff0099cc"
        android:src="@drawable/whisky" />

    <ImageButton
        android:id="@+id/BTNPIS"
        android:layout_width="315dp"
        android:layout_height="102dp"
        android:layout_below="@+id/textView12"
        android:layout_alignLeft="@+id/BTNCER"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="1dp"
        android:background="#ff0099cc"
        android:src="@drawable/pisco" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BTNPIS"
        android:layout_alignStart="@+id/BTNCER"
        android:layout_marginStart="115dp"
        android:layout_marginTop="0dp"
        android:text="ALERTA PATRIMONIO"
        android:textAlignment="center"
        android:textColor="#f4f4f4"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="147dp"
        android:layout_height="34dp"
        android:layout_below="@+id/BTNCER"
        android:layout_alignStart="@+id/BTNCER"
        android:layout_marginStart="91dp"
        android:layout_marginTop="4dp"
        android:text="INGRESO PATRIMONIO"
        android:textAlignment="center"
        android:textColor="#f4f4f4"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/BTNCER"
        android:layout_width="317dp"
        android:layout_height="108dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:background="#ff0099cc"
        android:src="@drawable/cerveza" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BTNCER"
        android:layout_alignStart="@+id/BTNWHI"
        android:layout_marginStart="15dp"
        android:layout_marginTop="300dp"
        android:text="TUTORIAL"
        android:textAlignment="center"
        android:textColor="#f4f4f4"
        android:textStyle="bold" />

</RelativeLayout>