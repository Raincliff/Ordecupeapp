<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ordecupe.android.principal">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="ordecupe.android.util.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Principal" />
        <activity android:name=".seguridad.PrincipalAdmin" />
        <activity android:name=".seguridad.PrincipalRep" />
        <activity android:name=".LoginPrincipal" />
        <activity
            android:name="ordecupe.android.entidades.Alertas"
            android:label="@string/title_activity_alertas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="ordecupe.android.entidades.patrimonio.RegistroCiudadano1"
            android:label="@string/title_activity_registro_ciudadano1"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>